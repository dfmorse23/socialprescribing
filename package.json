  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "railway": "run-s railway:*",
    "railway:1-generate": "prisma generate",
    "railway:2-migrate": "prisma migrate deploy",
    "railway:3-client": "NPM_CONFIG_PRODUCTION=false yarn --cwd ./client install && yarn --cwd ./client build",
    "build": "run-s build:*",
    "initialize": "yarn install && yarn --cwd client install && npx prisma migrate deploy && npx prisma generate"
  },
